{
  "name": "nxpm-stack",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "affected:deploy": "yarn affected --target deploy",
    "build": "yarn build:admin && yarn build:api",
    "build:admin": "nx build admin --prod",
    "build:api": "nx build api --prod",
    "dep-graph": "nx dep-graph",
    "dev:admin": "nx serve admin",
    "dev:api": "nx serve api",
    "docker:build": "docker build . -t nxpm-stack/api",
    "docker:push": "docker push nxpm-stack/api",
    "docker:run": "docker run -it -p 8000:3000 nxpm-stack/api",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "lint": "nx workspace-lint && nx lint",
    "nx": "nx",
    "prisma:apply": "yarn prisma:format && yarn prisma:migrate && yarn prisma:generate",
    "prisma:format": "yarn prisma format",
    "prisma:generate": "yarn prisma generate",
    "prisma:migrate": "yarn prisma migrate save --experimental && yarn prisma migrate up --experimental",
    "prisma:studio": "yarn prisma studio",
    "sdk": "graphql-codegen --config libs/admin/data-access-auth/src/codegen.yml",
    "sdk:watch": "yarn sdk --watch",
    "setup": "yarn prisma migrate save -n initial-migration --experimental && yarn prisma migrate up --experimental && yarn prisma:generate",
    "start": "node dist/apps/api/main.js",
    "test": "nx test",
    "test:ci": "yarn run test",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn format:check"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html,graphql}": [
      "yarn format --uncommitted"
    ],
    "nx.json": [
      "yarn nxpm-stack lint",
      "yarn format:write"
    ]
  },
  "dependencies": {
    "@angular/animations": "^10.1.0",
    "@angular/common": "^10.1.0",
    "@angular/compiler": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/forms": "^10.1.0",
    "@angular/platform-browser": "^10.1.0",
    "@angular/platform-browser-dynamic": "^10.1.0",
    "@angular/router": "^10.1.0",
    "@apollo/client": "^3.2.1",
    "@kikstart-playground/graphql-intercom": "1.4.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.7.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@ngrx/effects": "10.0.0",
    "@ngrx/entity": "10.0.0",
    "@ngrx/router-store": "10.0.0",
    "@ngrx/store": "10.0.0",
    "@ngx-formly/bootstrap": "5.10.3",
    "@ngx-formly/core": "5.10.3",
    "@prisma/cli": "^2.8.0",
    "@prisma/client": "^2.8.0",
    "apollo-angular": "^2.0.4",
    "apollo-server-express": "^2.18.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.2",
    "bootswatch": "^4.5.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.3",
    "graphql-type-json": "^0.3.2",
    "joi": "^17.2.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5",
    "supertest": "^5.0.0",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/compiler-cli": "^10.1.0",
    "@angular/language-service": "^10.1.0",
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-apollo-angular": "^2.0.1",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@ngrx/schematics": "10.0.0",
    "@ngrx/store-devtools": "10.0.0",
    "@nrwl/angular": "^10.4.4",
    "@nrwl/cli": "10.4.4",
    "@nrwl/cypress": "10.4.4",
    "@nrwl/eslint-plugin-nx": "10.4.4",
    "@nrwl/jest": "10.4.4",
    "@nrwl/nest": "^10.4.4",
    "@nrwl/node": "10.4.4",
    "@nrwl/tao": "10.4.4",
    "@nrwl/workspace": "10.4.4",
    "@nxpm/cli": "^1.9.0",
    "@nxpm/stack": "^2.3.1",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "~5.0.1",
    "cypress": "^5.5.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "husky": "^4.3.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "^10.4.0",
    "prettier": "2.1.2",
    "ts-jest": "26.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~4.0.3"
  },
  "prisma": {
    "schema": "libs/api/data-access-core/src/prisma/schema.prisma"
  }
}
